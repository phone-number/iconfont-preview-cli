{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "jsx": "preserve",
    "jsxImportSource": "vue" /* 从vue 3.4开始, vue不在隐式注册全局JSX命名空间 */,
    "lib": ["ES2018", "DOM", "DOM.Iterable"],
    "types": ["vite/client"],
    "skipLibCheck": true,
    "paths": {
      "/@/*": ["app/src/*"]
    }
  },
  "include": [
    "app/src/**/*",
    "app/auto-imports.d.ts",
    "app/components.d.ts"
  ]
}
