{
  "name": "iconfont-cli",
  "version": "1.0.0",
  "author": "wangzl",
  "description": "字体图标cli",
  "files": [
    "app",
    "components",
    "server"
  ],
  "scripts": {
    "dev:app": "pnpm run -C app dev",
    "dev:server": "pnpm run -C server dev",
    "dev": "pnpm run dev:server & pnpm run dev:app",
    "preview": "npx node dist/server/index.mjs --dir font-icon ",
    "build:app": "pnpm run -C app build",
    "build:other": "pnpm run -C build-other build",
    "build:server": "pnpm run -C server build",
    "build": "pnpm run -r --parallel build",
    "del:node_modules": "rimraf \"**/node_modules\"",
    "del:dist": "rimraf dist"
  },
  "keywords": [
    "iconfont",
    "字体图标",
    "字体图标预览",
    "字体图标vite插件"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.cjs"
    },
    "./components": {
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.cjs"
    },
    "./server": {
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.cjs"
    },
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components/types/index.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ]
    }
  },
  "bin": {
    "iconfont-cli": "./dist/server/bin.mjs"
  },
  "devDependencies": {
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=18"
  }
}
